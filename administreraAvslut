              anchor : "100%",
              style : "border-radius:3px",
              allowBlank : false,
              togglePanels : function () {
  var utkop = this.getFormField('fieldsetUtkop');
  var internforsaljning = this.getFormField('fieldsetInternforsaljning').getValue();
  var vadHander = this.getFormField('vadHander');   
  
    if (vadHander == 'Säljs Internt')
      {
          internforsaljning.show();
          internforsaljning.validate();        
     }
      else
        {
          internforsaljning.hide();
          internforsaljning.validate();          
     }  
   
       if (vadHander == 'Köps ut')
      {
          utkop.show();
          utkop.validate();        
     }
      else
        {
          utkop.hide();
          utkop.validate();          
     }  
 
},
              listeners : {
  render : function (combo) {
var form = combo.findParentByType("jsonformpanel");
if(!form) 
    form = Ext.getCmp("presentpanel");
if (form)
    form.on("afterjsonload",function() {combo.togglePanels(combo);});
},
  select : function (combo) {
combo.getFormField("utkop").setValue("");
combo.getFormField("internForsaljning").setValue("");
combo.togglePanels(combo); 
    }
}
